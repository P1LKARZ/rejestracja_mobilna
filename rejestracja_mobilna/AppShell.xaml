<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="rejestracja_mobilna.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:rejestracja_mobilna"
    Shell.FlyoutBehavior="Disabled"
    Title="rejestracja_mobilna">

    <ShellContent
        Title="Rejestracja"
        ContentTemplate="{DataTemplate local:MainPage}"
        Route="MainPage" 
 />
 <ShellContent
        Title="Druga strona"
        ContentTemplate="{DataTemplate local:ReloadPage}"
        Route="ReloadPage" 
 />


</Shell>
